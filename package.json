{
  "name": "mindmap-mental-health-platform",
  "version": "2.0.0",
  "description": "Research-grade mental health tracking platform with AI analytics and geographic mapping",
  "main": "backend/src/server.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cd frontend && live-server --port=3000",
    "dev:backend": "cd backend && nodemon src/server.js",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "cd frontend && webpack --mode=production",
    "build:backend": "cd backend && npm run build",
    "test": "concurrently \"npm run test:frontend\" \"npm run test:backend\"",
    "test:frontend": "cd frontend && jest",
    "test:backend": "cd backend && jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "start": "node backend/src/server.js",
    "docker:up": "docker-compose up",
    "docker:down": "docker-compose down",
    "jupyter": "docker-compose up data-science"
  },
  "keywords": [
    "mental-health",
    "research",
    "psychology",
    "ai",
    "machine-learning",
    "geographic-mapping",
    "data-science",
    "docker",
    "postgresql",
    "firebase"
  ],
  "author": "MindMap Research Team",
  "license": "MIT",
  "dependencies": {
    "framer-motion": "^12.23.11",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "firebase-admin": "^11.11.1",
    "pg": "^8.11.3",
    "sequelize": "^6.33.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "joi": "^17.11.0",
    "moment": "^2.29.4",
    "chart.js": "^4.4.0",
    "leaflet": "^1.9.4",
    "@tensorflow/tfjs": "^4.13.0",
    "axios": "^1.5.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.2",
    "live-server": "^1.2.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "jest": "^29.7.0",
    "eslint": "^8.52.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2",
    "@types/node": "^20.8.6",
    "@types/express": "^4.17.20",
    "@types/cors": "^2.8.15",
    "@types/bcryptjs": "^2.4.5",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/pg": "^8.10.7"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}